/
&tGrid
  nt_opcyc=1024,         !NUMBER OF TIMEFRAMES WITHIN OPTIC CYCLE [INT, POWER OF 2]
  ntps=24000,            !NUMBER OF TIMEFRAMES IN DYNAMIC [INT]
  np_hhg=8,              !NUMBER OF POINTS BETWEEN EACH MULTIPLE OF THE FREQUENCY OF THE FIELD [INT]
/
&fieldParameters
  t0=0d0,                !DYNAMIC BEGINNING [REAL]
  e0=3.77d-2,            !SQRT FIELD INTENSITY IN A.U. [REAL]
  freq=4.3d-3,           !FIELD FREQUENCY IN A.U. [REAL]
  fph=0.E0,               !FIELD PHASE IN 2.PI.RAD [REAL]
/
